<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ActualizarUsuario</servlet-name>
        <servlet-class>Servlets.ActualizarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarUsuario</servlet-name>
        <servlet-class>Servlets.AgregarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarUsuario</servlet-name>
        <servlet-class>Servlets.EliminarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Servlets.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ObtenerUsuarios</servlet-name>
        <servlet-class>Servlets.ObtenerUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerificarSesion</servlet-name>
        <servlet-class>Servlets.VerificarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ObtenerUsuario</servlet-name>
        <servlet-class>Servlets.ObtenerUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActualizarUsuario</servlet-name>
        <url-pattern>/ActualizarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarUsuario</servlet-name>
        <url-pattern>/AgregarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarUsuario</servlet-name>
        <url-pattern>/EliminarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObtenerUsuarios</servlet-name>
        <url-pattern>/ObtenerUsuarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerificarSesion</servlet-name>
        <url-pattern>/VerificarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ObtenerUsuario</servlet-name>
        <url-pattern>/ObtenerUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
